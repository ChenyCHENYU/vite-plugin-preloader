{
  "name": "vite-plugin-preloader",
  "version": "1.1.0",
  "description": "🚀 Vite plugin for intelligent route preloading - 智能路由预加载插件",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "vite",
    "vite-plugin",
    "preload",
    "vue",
    "vue3",
    "performance",
    "route",
    "lazy-loading",
    "optimization",
    "build-tool",
    "intelligent",
    "preloading"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^7.0.0 || ^6.0.0 || ^5.0.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.21.0",
    "prettier": "^3.5.3",
    "tsup": "^8.3.5",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vitest": "^3.0.8",
    "vue": "^3.5.13"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": {
    "name": "ChenYu",
    "email": "ycyplus@gmail.com",
    "url": "https://github.com/ChenyCHENYU"
  },
  "license": "MIT",
  "homepage": "https://github.com/ChenyCHENYU/vite-plugin-preloader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChenyCHENYU/vite-plugin-preloader.git"
  },
  "bugs": {
    "url": "https://github.com/ChenyCHENYU/vite-plugin-preloader/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
